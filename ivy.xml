		<dependency org="prom" name="ProM-Framework" rev="latest" changing="true" transitive="true" conf="compile->default;runtime->default"/>
		<dependency org="prom" name="ProM-Plugins" rev="latest" changing="true" transitive="true"/>
        <dependency org="prom" name="ProM-Contexts" rev="latest" changing="true" transitive="true"/>
