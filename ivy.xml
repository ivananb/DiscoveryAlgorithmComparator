		<dependency org="prom" name="GraphVisualizers" rev="latest" conf="compile->default;runtime->default"/>
		<dependency org="prom" name="ProM-Framework" rev="latest" conf="compile->default;runtime->default"/>
		<dependency org="prom" name="DottedChart" rev="latest" conf="compile->default;runtime->default"/>
		<dependency org="prom" name="DottedChart2" rev="latest" conf="compile->default;runtime->default"/>
		<dependency org="prom" name="Statechart" rev="latest" conf="compile->default;runtime->default"/>
		
		<dependency org="prom" name="ChangeVisualization" rev="latest" conf="compile->default;runtime->default"/>
		<dependency org="prom" name="DeclareMinerVisualizer" rev="latest" conf="compile->default;runtime->default"/>
